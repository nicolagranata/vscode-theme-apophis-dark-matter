    "tokenColors": [
        {
            "name": "Comment text",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#81a1c4cc"
            }
        },
        {
            "name": "Comment punctuation",
            "scope": "punctuation.definition.comment, punctuation.whitespace.comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#647484cc"
            }
        },
        {
            "name": "String",
            "scope": "string, string.unquoted",
            "settings": {
                "foreground": "#909f80ff"
            }
        },
        {
            "name": "Heredoc",
            "scope": "string.unquoted.heredoc",
            "settings": {
                "foreground": "#d6e6d6ff"
            }
        },
        {
            "name": "phpdoc",
            "scope": "keyword.other.phpdoc, storage.type.class.jsdoc, comment.block.documentantion.ts",
            "settings": {
                "foreground": "#d1c516ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "embedded ruby in string delimiters",
            "scope": "punctuation.section.embedded",
            "settings": {
                "foreground": "#e68494ff"
            }
        },
        {
            "name": "embedded ruby in string",
            "scope": "source.ruby.embedded",
            "settings": {
